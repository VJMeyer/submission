@article{olsenDiplomatsDomesticAgency2020,
  title = {Diplomats, {{Domestic Agency}} and the {{Implementation}} of {{Sanctions}}: {{The MFAs}} of {{France}} and {{Germany}} in the {{Age}} of {{Geoeconomic Diplomacy}}},
  shorttitle = {Diplomats, {{Domestic Agency}} and the {{Implementation}} of {{Sanctions}}},
  author = {Olsen, Kim B.},
  date = {2020-03-10},
  journaltitle = {The Hague Journal of Diplomacy},
  shortjournal = {Hague J. Dipl.},
  volume = {15},
