@article{divito2022,
  title = {A {{Literature Review}} on {{Corporate Governance Mechanisms}}: {{Past}}, {{Present}}, and {{Future}}*},
  shorttitle = {A {{Literature Review}} on {{Corporate Governance Mechanisms}}},
  author = {Di Vito, Jackie and Trottier, Kim},
  date = {2022-06},
  journaltitle = {Accounting Perspectives},
  shortjournal = {Accounting Perspectives},
  volume = {21},
  number = {2},
  pages = {207--235},
  issn = {1911-382X, 1911-3838},
  doi = {10.1111/1911-3838.12279},
  url = {https://onlinelibrary.wiley.com/doi/10.1111/1911-3838.12279},
  urldate = {2025-05-26},
  abstract = {ABSTRACT             This study is a literature review on corporate governance. Its objective is to consolidate our knowledge in this field, examine its evolution, and propose avenues for future research. In our review of the past and present literature on various governance measures and their effect on firm performance, we find that the empirical results are mixed for many of the governance mechanisms studied. We propose that these mixed results may be due to applying a “one size fits all” set of governance measures, which is not effective for all types of firms due to the complexity of organizations and the differences in ownership structures. We therefore explore more technologically advanced methodologies, including machine learning. We believe that this line of research could not only improve and refine existing governance measures but also allow us to better target which set of mechanisms might be appropriate for a firm based on its particular characteristics. We encourage future researchers in corporate governance to consider this approach in order to shed light on and fill the gaps in this area of research.           ,              RÉSUMÉ                            Analyse documentaire sur les mécanismes de gouvernance d'entreprise~: passé, présent et futur                          La présente étude est une revue de littérature sur la gouvernance d'entreprise qui vise à consolider nos connaissances dans ce domaine, à examiner son évolution et à proposer des pistes de recherches futures. Dans le cadre de notre analyse de la littérature passée et actuelle sur les diverses mesures de gouvernance et leur effet sur la performance des entreprises, nous constatons que les résultats empiriques sont mitigés pour la plupart des mécanismes de gouvernance examinés. Nous suggérons que ces résultats mitigés sont attribuables à l'application d'un ensemble de mesures de gouvernance universelles dont l'efficacité n'est pas la même pour tous les types d'entreprises compte tenu de la complexité des organisations et des différences entre les structures de propriété. Nous explorons donc des méthodologies plus avancées sur le plan technologique, y compris l'apprentissage machine. Nous croyons que cet angle de recherche permettrait non seulement d'améliorer et d'affiner les mesures de gouvernance existantes, mais également de mieux cibler les mécanismes qui pourraient être appropriés pour une entreprise donnée en fonction de ses caractéristiques particulières. Nous encourageons les chercheurs qui étudieront la gouvernance d'entreprise à considérer cette approche méthodologique afin d'améliorer la recherche et de combler les lacunes dans ce domaine de recherche.},
  langid = {english},
  file = {/Users/vjm/Zotero/storage/6RXBE64T/Di Vito and Trottier - 2022 - A Literature Review on Corporate Governance Mechanisms Past, Present, and Future.pdf}
}

